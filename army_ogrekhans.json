  "magicitems": {
    "weapons": [
      {
        "name": "Glory of the Dawn Age",
        "cost": 100,
        "description": "Spear enchantment. Attacks made with this weapon gain +1 Strength, Magical Attacks, and Multiple Wounds (2)."
      },
      {
        "name": "Wildfire Burs",
        "cost": 55,
        "description": "Bow enchantment. This weapon has Shots 4, Str 4, AP 1. Attacks made with this weapon gain Flaming Attacks and Magical Attacks. A unit that is hit by these attacks loses Soft Cover until the end of the Player Turn (if it had it). If the enemy unit was in Hard Cover, it is now considered to be in Soft Cover until the end of the Player Turn."
      }
    ],
    "armours": [
      {
        "name": "Taurosaur's Vigour",
        "cost": 70,
        "description": "Suit of Armour enchantment. The wearer gains +1 Armour, +1 Health Point, and Crush Attack."
      },
      {
        "name": "Serrate Scales",
        "cost": 50,
        "description": "Shield enchantment. The bearer gains +2 Attack Value while using this Shield. The 2 Close Combat Attacks gained by this enchantment always have Strength 5 and Armour Penetration 2. These attacks do not benefit from any rules or bonuses granted by Weapon Enchantments"
      }
    ],
    "artefacts": [
      {
        "name": "Sphere of Shielding",
        "cost": 100,
        "description": "Models on Palanquin and Skink Palanquin only. The bearer gains Aegis (2+, against Ranged Attacks). The bearer may not take saves of any kind against Melee Attacks."
      },
      {
        "name": "Ancient Plaque",
        "cost": 85,
        "dominant" : 1,
        "description": "Once per friendly Magic Phase, the bearer may reroll one Magic Dice that was used for a Casting Attempt. This ability cannot be used when the bearer is casting with only one Magic Dice, and cannot reroll a dice which is one, of three or more rolling the same value."
      },
      {
        "name": "Starfall Shard ",
        "cost": 70,
        "description": "The bearer’s model gains Aegis (2+, against Flaming Attacks) and Hard Target, and cannot benefit from Regeneration."
      },
      {
        "name": "Spirit of the Stampede",
        "cost": 70,
        "description": "The bearer’s mount gains Impact Hits (D6). If it already had Impact Hits, increase the number of Impact Hits by D6."
      },
      {
        "name": "Jade Staff",
        "cost": 55,
        "description": "The bearer can cast Healing Waters from Druidism as a Bound Spell with Power Level (4/8)."
      },
      {
        "name": "Raptor Spirit",
        "cost": 65,
        "description": "Models on foot only. The bearer gains +4 Advance Rate, +4 March Rate, and Swiftstride. This Artefact cannot be taken by a model with Ranger’s Boots."
      },
      {
        "name": "Egg of the Quetza",
        "cost": 45,
        "description": "The bearer gains Breath Attack (Str D3+2, AP 1, Magical Attacks)."
      },
      {
        "name": "Sun Tablet",
        "cost": 40,
        "description": "The bearer can select its spells from the Learned Spells 1, 2, 3, 4, 5, and 6 of its chosen Path and the Hereditary Spell of its army. This overrides the normal Spell Selection rules connected to being a Wizard Apprentice/Adept. This Artefact cannot be combined with Protean Potentate."
      },
      {
        "name": "Obsidian Tesseract",
        "cost": 35,
        "description": "One use only. May be activated in the opponent's Magic Phase after Siphon the Veil. When activated, remove 1 Magic Dice from the opponent’s Magic Dice pool."
      }
    ],
    "standards": [
      {
        "name": "Totem of Mixoatl",
        "cost": 75,
        "description": "Cannot be taken by Core units. The bearer's unit gains Hard Target. One use only. May be activated at the start of a Round of Combat. Until the end of the Round of Combat, all enemy units Engaged with the bearer's unit suffer -3 Offensive Skill."
      },
      {
        "name": "Tree Frog Banner",
        "cost": 40,
        "description": "Close Combat Attacks from Skink* model parts in the unit gain Poison Attacks."
      }
    ]
